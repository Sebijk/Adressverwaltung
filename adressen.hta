<body dir="ltr" scroll="auto" background="data/background.jpg" bgproperties="fixed">
<script type="text/javascript" language="JavaScript">

	 IE = typeof(document.all)=='object';
	 
		function onButtonClick(itemId,itemValue)
		{
		if (itemId == "help_about") { if(IE){ info(); } }
		if (itemId == "help_index") { if(IE){ help(); } }
		if (itemId == "help_license") { if(IE){ show_license(); } }
    if (itemId == "print") { if(IE){ printer(); } }
    if (itemId == "find") { if(IE){ search(); } }
    if (itemId == "edit_clear") { if(IE){ del(); } }
    if (itemId == "edit_clearquery") { if(IE){ deletequery(); } }
    if (itemId == "save") { if(IE){ backup(); } }
    if (itemId == "saveastxt") { if(IE){ saveastxt(); } }
    if (itemId == "close") { if(IE){ OnQuit(); } }
    if (itemId == "restoreadr") { if(IE){ restore(); } }
    if (itemId == "add")  { if(IE){ add(); } }
    if (itemId == "reload") { if(IE){ reload(); } }
    }
		
		aMenuBar=new dhtmlXMenuBarObject('menu_zone',600,20,"",0);
		aMenuBar.setGfxPath("data/codebase/imgs/");		
		aMenuBar.setOnClickHandler(onButtonClick);
		aMenuBar.loadXML("data/_menu.xml")
		aMenuBar.showBar();

</script><br />
